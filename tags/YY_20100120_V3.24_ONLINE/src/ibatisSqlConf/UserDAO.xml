<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="UserDAO">

	<typeAlias alias="userParamter"
		type="com.china.centet.yongyin.bean.User" />

	<resultMap id="userMap"
		class="com.china.centet.yongyin.bean.User">
		<result property="id" column="id" />
		<result property="name" column="name" />
		<result property="password" column="password" />
		<result property="stafferName" column="stafferName" />
		<result property="locationID" column="locationID" />
		<result property="type" column="role" />
		<result property="status" column="status" />
		<result property="fail" column="fail" />
		<result property="loginTime" column="loginTime" />
	</resultMap>

	<select id="queryCommon" parameterClass="java.util.Map"
		resultMap="userMap">
		<![CDATA[
    select * From t_center_user Where role <> 3 and name <> #name#
    ]]>
	</select>

</sqlMap>
