<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
		 <action path="/customer/customer" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false">
			<forward name="queryCustomer"
				path="/customer/queryCustomer.jsp" />
			<forward name="queryApplyCustomer"
				path="/customer/queryApplyCustomer.jsp" />
		    <forward name="queryApplyCustomerForCode"
                path="/customer/queryApplyCustomerForCode.jsp" />
            <forward name="queryApplyCustomerForCredit"
                path="/customer/queryApplyCustomerForCredit.jsp" />
			<forward name="queryAssignCustomerApply"
				path="/customer/queryAssignCustomerApply.jsp" />
			<forward name="querySelfCustomer"
				path="/customer/querySelfCustomer.jsp" />
			<forward name="updateCustomer"
				path="/customer/updateCustomer.jsp" />
			<forward name="addCustomer"
				path="/customer/addCustomer.jsp" />
			<forward name="detailCustomer"
				path="/customer/detailCustomer.jsp" />
			<forward name="uploadCustomer"
				path="/customer/uploadCustomer.jsp" />
			<forward name="queryCustomerDistribute"
				path="/customer/queryCustomerDistribute.jsp" />
		</action>
		
		<action path="/customer/check" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false">
			<forward name="queryCheckCustomer"
				path="/customer/queryCheckCustomer.jsp" />
			<forward name="addCheckCustomer"
				path="/customer/addCheckCustomer.jsp" />
			<forward name="updateCheckCustomer"
				path="/customer/updateCheckCustomer.jsp" />
			<forward name="detailCheckCustomer"
				path="/customer/detailCheckCustomer.jsp" />
		</action>
		
		<action path="/credit/credit" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="queryCreditItem" path="/credit/queryCreditItem.jsp" />
            <forward name="queryCreditItemSec" path="/credit/queryCreditItemSec.jsp" />
            <forward name="queryCreditItemThr" path="/credit/queryCreditItemThr.jsp" />
            <forward name="addCreditItemThr" path="/credit/addCreditItemThr.jsp" />
            <forward name="updateCreditItemThr" path="/credit/updateCreditItemThr.jsp" />
            <forward name="queryCreditLevel" path="/credit/queryCreditLevel.jsp" />
        </action>
        
        <action path="/credit/customer" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="querySelfCustomer"
                path="/customer/querySelfCustomer.jsp" />
            <forward name="configCustomerCredit" path="/credit/configCustomerCredit.jsp" />
            <forward name="detailCustomerCredit" path="/credit/detailCustomerCredit.jsp" />
            <forward name="interposeCredit" path="/credit/interposeCredit.jsp" />
            <forward name="querySelfCustomer" path="/customer/querySelfCustomer.jsp" />
            <forward name="uploadCustomerCredit" path="/credit/uploadCustomerCredit.jsp" />
            <forward name="queryApplyCustomerForCredit" path="/customer/queryApplyCustomerForCredit.jsp" />
        </action>
		
	</action-mappings>
</struts-config>