<?xml version="1.0" encoding="UTF-8"?>
<query>
	<item name="queryBudget" deaultpfix="BudgetBean">
        <condtion name="name" caption="预算名称">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="stafferId" caption="提交人">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$staffer</option>
        </condtion>
        <condtion name="status" caption="预算状态">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$budgetStatus</option>
        </condtion>
    </item>
    
    <item name="queryBudgetForApprove" deaultpfix="BudgetBean">
        <condtion name="name" caption="预算名称">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="stafferId" caption="提交人">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$staffer</option>
        </condtion>
        <condtion name="status" caption="预算状态">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$budgetStatus</option>
        </condtion>
    </item>
    
    <item name="queryBudgetLog" deaultpfix="BudgetLogBean">
        <condtion name="alogTime" filed="logTime" caption="开始时间">
            <opr>&gt;=</opr>
            <type>datetime</type>
            <inner></inner>
        </condtion>
        <condtion name="blogTime" filed="logTime" caption="结束时间">
            <opr>&lt;=</opr>
            <type>datetime</type>
            <inner></inner>
        </condtion>
        <condtion name="budgetId" caption="预算">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$releaseBudget</option>
        </condtion>
        <condtion name="billId" caption="付款单">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="feeItemId" caption="预算项">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$feeItemList</option>
        </condtion>
    </item>
	
</query>
