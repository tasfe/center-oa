<?xml version="1.0" encoding="UTF-8"?>
<query>
	
	<item name="queryProduct" deaultpfix="ProductBean">
		<condtion name="name" caption="名称">
			<opr>like</opr>
			<type>text</type>
		</condtion>
		<condtion name="name" caption="编码">
			<opr>code</opr>
			<type>text</type>
		</condtion>
		<condtion name="status" caption="状态">
			<opr>=</opr>
			<datatype>1</datatype>
			<type>select</type>
			<option>$productStatus</option>
		</condtion>
		<condtion name="type" caption="类型">
			<opr>=</opr>
			<datatype>1</datatype>
			<type>select</type>
			<option>$productType</option>
		</condtion>
		<condtion name="abstractType" caption="虚拟产品">
			<opr>=</opr>
			<datatype>1</datatype>
			<type>select</type>
			<option>$productAbstractType</option>
		</condtion>
		<condtion name="stockType" caption="库存模型">
			<opr>=</opr>
			<datatype>1</datatype>
			<type>select</type>
			<option>$productStockType</option>
		</condtion>
	</item>

</query>
