<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
		 <action path="/product/product" parameter="method" folder="/product/" bean="Product"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="configProductVSLocation"/>
            <forward name="rptQueryProduct"/>
            <forward name="queryApplyProduct"/>
        </action>
        
		<action path="/provider/provider" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false">
			<forward name="queryProvider" path="/provider/queryProvider.jsp" />
			<forward name="bingProductType" path="/provider/bingProductType.jsp" />
			<forward name="rptQueryProvider" path="/provider/rptQueryProvider.jsp" />
			<forward name="addOrUpdateProviderUser" path="/provider/addOrUpdateProviderUser.jsp" />
			<forward name="updateProvider"
				path="/provider/updateProvider.jsp" />
			<forward name="detailProvider"
				path="/provider/detailProvider.jsp" />
			<forward name="addOrUpdateUser"
                path="/provider/addOrUpdateUser.jsp" />
		</action>
		
		<action path="/depot/depot" parameter="method" folder="/depot/" bean="Depot"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
        </action>
        
        <action path="/depot/depotpart" parameter="method" folder="/depot/" bean="Depotpart"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
        </action>
        
        <action path="/depot/storage" parameter="method" folder="/depot/" bean="Storage"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
        </action>
		
	</action-mappings>
</struts-config>