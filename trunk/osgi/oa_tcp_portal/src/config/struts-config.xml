<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
	
		<action path="/tcp/apply" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false">
			<forward name="addTravelApply0" path="/tcp/addTravelApply0.jsp" />
			<forward name="addTravelApply1" path="/tcp/addTravelApply1.jsp" />
			<forward name="addTravelApply2" path="/tcp/addTravelApply2.jsp" />
			<forward name="addTravelApply3" path="/tcp/addTravelApply3.jsp" />
			
			<forward name="updateTravelApply0" path="/tcp/updateTravelApply0.jsp" />
			<forward name="updateTravelApply1" path="/tcp/updateTravelApply1.jsp" />
			<forward name="updateTravelApply2" path="/tcp/updateTravelApply2.jsp" />
			<forward name="updateTravelApply3" path="/tcp/updateTravelApply3.jsp" />
			
			<forward name="queryTravelApply" path="/tcp/queryTravelApply.jsp" />
			
			<forward name="querySelfTravelApply0" path="/tcp/querySelfTravelApply0.jsp" />
			<forward name="querySelfTravelApply1" path="/tcp/querySelfTravelApply1.jsp" />
			<forward name="querySelfTravelApply2" path="/tcp/querySelfTravelApply2.jsp" />
			<forward name="querySelfTravelApply3" path="/tcp/querySelfTravelApply3.jsp" />
			
			<forward name="detailTravelApply0" path="/tcp/detailTravelApply0.jsp" />
			<forward name="detailTravelApply1" path="/tcp/detailTravelApply1.jsp" />
			<forward name="detailTravelApply2" path="/tcp/detailTravelApply2.jsp" />
			<forward name="detailTravelApply3" path="/tcp/detailTravelApply3.jsp" />
			
			<forward name="processTravelApply0" path="/tcp/processTravelApply0.jsp" />
			<forward name="processTravelApply1" path="/tcp/processTravelApply1.jsp" />
			<forward name="processTravelApply2" path="/tcp/processTravelApply2.jsp" />
			<forward name="processTravelApply3" path="/tcp/processTravelApply3.jsp" />
			
			<forward name="querySelfApprove" path="/tcp/querySelfApprove.jsp" />
		</action>
		
		<action path="/tcp/expense" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="addExpense0" path="/expense/addExpense0.jsp" />
            <forward name="addExpense1" path="/expense/addExpense1.jsp" />
            <forward name="addExpense2" path="/expense/addExpense2.jsp" />
            <forward name="addExpense3" path="/expense/addExpense3.jsp" />
            
            <forward name="updateExpense0" path="/expense/updateExpense0.jsp" />
            <forward name="updateExpense1" path="/expense/updateExpense1.jsp" />
            <forward name="updateExpense2" path="/expense/updateExpense2.jsp" />
            <forward name="updateExpense3" path="/expense/updateExpense3.jsp" />
            
            <forward name="queryExpense" path="/expense/queryExpense.jsp" />
            
            <forward name="querySelfExpense0" path="/expense/querySelfExpense0.jsp" />
            <forward name="querySelfExpense1" path="/expense/querySelfExpense1.jsp" />
            <forward name="querySelfExpense2" path="/expense/querySelfExpense2.jsp" />
            <forward name="querySelfExpense3" path="/expense/querySelfExpense3.jsp" />
            
            <forward name="detailExpense0" path="/expense/detailExpense0.jsp" />
            <forward name="detailExpense1" path="/expense/detailExpense1.jsp" />
            <forward name="detailExpense2" path="/expense/detailExpense2.jsp" />
            <forward name="detailExpense3" path="/expense/detailExpense3.jsp" />
            
            <forward name="processExpense0" path="/expense/processExpense0.jsp" />
            <forward name="processExpense1" path="/expense/processExpense1.jsp" />
            <forward name="processExpense2" path="/expense/processExpense2.jsp" />
            <forward name="processExpense3" path="/expense/processExpense3.jsp" />
        </action>
		
	</action-mappings>
</struts-config>