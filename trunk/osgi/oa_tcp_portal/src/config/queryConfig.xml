<?xml version="1.0" encoding="UTF-8"?>
<query>
    
    <item name="querySelfTravelApply" deaultpfix="TravelApplyBean">
        <condtion name="alogTime" filed="logTime" caption="日期从">
            <opr>&gt;=</opr>
            <type>date</type>
        </condtion>
        <condtion name="blogTime" filed="logTime" caption="到">
            <opr>&lt;=</opr>
            <type>date</type>
        </condtion>
        <condtion name="name" caption="目的">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="status" caption="状态">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$tcpStatus</option>
        </condtion>
         <condtion name="borrow" caption="借款">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$travelApplyBorrow</option>
        </condtion>
    </item>
    
    <item name="querySelfApprove" deaultpfix="TcpApproveBean">
        <condtion name="alogTime" filed="logTime" caption="日期从">
            <opr>&gt;=</opr>
            <type>date</type>
        </condtion>
        <condtion name="blogTime" filed="logTime" caption="到">
            <opr>&lt;=</opr>
            <type>date</type>
        </condtion>
        <condtion name="applyId" caption="标识(A)">
            <opr>=</opr>
            <type>text</type>
        </condtion>
        <condtion name="name" caption="目的">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="status" caption="状态">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$tcpStatus</option>
        </condtion>
         <condtion name="type" caption="类型">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$tcpType</option>
        </condtion>
         <condtion name="applyerId" filed="name" caption="申请人" pfix="APPLYER">
             <opr>like</opr>
            <type>text</type>
        </condtion>
    </item>
    
    <item name="queryPoolApprove" alias="querySelfApprove"/>
    
</query>
