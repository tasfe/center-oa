<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
		<action path="/finance/bank" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="addBank" path="/bank/addBank.jsp" />
            <forward name="updateBank" path="/bank/updateBank.jsp" />
            <forward name="queryBank" path="/bank/queryBank.jsp" />
            <forward name="rptQueryBank" path="/bank/rptQueryBank.jsp" />
            <forward name="uploadPayment" path="/bank/uploadPayment.jsp" />
        </action>
        
        <action path="/finance/invoiceins" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="addInvoiceins" path="/invoiceins/addInvoiceins.jsp" />
            <forward name="queryInvoiceins" path="/invoiceins/queryInvoiceins.jsp" />
        </action>
		
	</action-mappings>
</struts-config>