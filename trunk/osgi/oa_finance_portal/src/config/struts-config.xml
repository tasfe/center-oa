<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
		<action path="/finance/bank" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="addBank" path="/bank/addBank.jsp" />
            <forward name="updateBank" path="/bank/updateBank.jsp" />
            <forward name="queryBank" path="/bank/queryBank.jsp" />
            <forward name="rptQueryBank" path="/bank/rptQueryBank.jsp" />
            <forward name="uploadPayment" path="/bank/uploadPayment.jsp" />
            <forward name="drawPayment" path="/bank/drawPayment.jsp" />
            <forward name="drawPayment2" path="/bank/drawPayment2.jsp" />
            <forward name="querySelfPayment" path="/bank/querySelfPayment.jsp" />
            <forward name="detailPaymentApply" path="/bank/detailPaymentApply.jsp" />
            <forward name="handlePaymentApply" path="/bank/handlePaymentApply.jsp" />
            <forward name="queryPaymentApply" path="/bank/queryPaymentApply.jsp" />
            <forward name="querySelfPaymentApply" path="/bank/querySelfPaymentApply.jsp" />
            <forward name="refBill" path="/bank/refBill.jsp" />
        </action>
        
        <action path="/finance/invoiceins" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="addInvoiceins" path="/invoiceins/addInvoiceins.jsp" />
            <forward name="queryInvoiceins" path="/invoiceins/queryInvoiceins.jsp" />
            <forward name="detailInvoiceins" path="/invoiceins/detailInvoiceins.jsp" />
        </action>
        
        <action path="/finance/bill" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="queryInBill" path="/bill/queryInBill.jsp" />
            <forward name="detailInBill" path="/bill/detailInBill.jsp" />
        </action>
		
	</action-mappings>
</struts-config>