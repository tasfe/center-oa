<?xml version="1.0" encoding="ISO-8859-1"?>
<struts-config>
	<action-mappings>
		 <action path="/sail/out" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false">
			<forward name="addOut" path="/sail/addOut.jsp" />
			<forward name="addOut1" path="/sail/addOut1.jsp" />
			<forward name="updateOut" path="/sail/updateOut.jsp" />
			<forward name="querySelfOut" path="/sail/querySelfOut.jsp" />
			<forward name="queryOut" path="/sail/queryOut.jsp" />
			<forward name="listOut1" path="/sail/listOut1.jsp" />
			<forward name="detailOut" path="/sail/detailOut.jsp" />
			<forward name="print" path="/sail/print.jsp" />
			<forward name="addOutBalance" path="/sail/addOutBalance.jsp" />
			<forward name="queryOutBalance" path="/sail/queryOutBalance.jsp" />
			<forward name="detailOutBalance" path="/sail/detailOutBalance.jsp" />
			<forward name="addBuy" path="/sail/addBuy.jsp" />
			<forward name="querySelfBuy" path="/sail/querySelfBuy.jsp" />
			<forward name="updateBuy" path="/sail/updateBuy.jsp" />
			<forward name="detailBuy" path="/sail/detailBuy.jsp" />
			<forward name="queryBuy" path="/sail/queryBuy.jsp" />
			<forward name="handerInvokeBuy" path="/sail/handerInvokeBuy.jsp" />
			<forward name="queryWarnOut" path="/sail/queryWarnOut.jsp" />
			<forward name="rptQueryOut" path="/sail/rptQueryOut.jsp" />
		</action>
		
		<action path="/sail/transport" parameter="method"
            type="org.springframework.web.struts.DelegatingActionProxy"
            validate="false">
            <forward name="transportList"
                path="/transport/listTransport.jsp" />
            <forward name="addTransport" path="/transport/addTransport.jsp" />
            <forward name="queryConsign" path="/transport/queryConsign.jsp" />
            <forward name="print1" path="/transport/print1.jsp" />
            <forward name="detailOut5" path="/transport/handleConsign.jsp" />
            <forward name="queryOut" path="/sail/queryOut.jsp" />
            <forward name="queryTodayConsign" path="/transport/queryTodayConsign.jsp" />
        </action>
		
	</action-mappings>
</struts-config>