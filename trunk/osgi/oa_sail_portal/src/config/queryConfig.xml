<?xml version="1.0" encoding="UTF-8"?>
<query>
    
    <item name="queryOut" deaultpfix="OutBean">
        <condtion name="name" caption="指标项">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="face" caption="类型" pfix="CreditItemSecBean">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$creditItemFace</option>
        </condtion>
        <condtion name="pid" caption="父项">
            <opr>=</opr>
            <datatype>0</datatype>
            <type>select</type>
            <option>$creditItemSecList</option>
        </condtion>
    </item>
    
    <item name="querySailConfig" deaultpfix="SailConfigBean">
        <condtion name="showName" filed="name" caption="品名" pfix="ShowBean">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="sailType" caption="销售类型">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$productSailType</option>
        </condtion>
        <condtion name="productType" caption="销售品类">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$productType</option>
        </condtion>
    </item>
	
</query>
