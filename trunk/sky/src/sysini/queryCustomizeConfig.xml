<?xml version="1.0" encoding="UTF-8"?>
<query>
    <item name="querySelfMake" deaultpfix="MakeBean">
        <condtion name="alogTime" filed="logTime" caption="开始时间">
            <opr>&gt;=</opr>
            <type>date</type>
            <inner></inner>
        </condtion>
        <condtion name="blogTime" filed="logTime" caption="结束时间">
            <opr>&lt;=</opr>
            <type>date</type>
            <inner></inner>
        </condtion>
        <condtion name="title" caption="主题">
            <opr>like</opr>
            <type>text</type>
        </condtion>
        <condtion name="type" caption="定制类型">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$makeType</option>
        </condtion>
        <condtion name="createrId" caption="创建人">
            <opr>=</opr>
            <datatype>1</datatype>
            <type>select</type>
            <option>$staffer</option>
        </condtion>
    </item>
    
