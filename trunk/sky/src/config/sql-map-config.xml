	<sqlMap resource="ibatisSqlConf/WorkLogDAO.xml" />
	<sqlMap resource="ibatisSqlConf/SnapshotDAO.xml" />
	<sqlMap resource="ibatisSqlConf/OutStatDAO.xml" />
