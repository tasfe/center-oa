<ehcache>
	<defaultCache maxElementsInMemory="200" eternal="false"
		timeToIdleSeconds="1800" timeToLiveSeconds="0" overflowToDisk="false" />

	<diskStore path="java.io.tmpdir" />

	<cache name="CACHE_DEFAULT_TEMPLATE" maxElementsInMemory="0"
		eternal="true" timeToIdleSeconds="0" timeToLiveSeconds="7200"
		overflowToDisk="true" maxElementsOnDisk="10000" diskPersistent="true"
		diskSpoolBufferSizeMB="10" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="CENTER_CHINA_CACHE_LOCAL" maxElementsInMemory="0"
		eternal="true" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="true" maxElementsOnDisk="3000" diskPersistent="true"
		diskSpoolBufferSizeMB="10" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />
</ehcache>